    "start": "next start -p 3001",
    "codegen": "graphql-codegen --config codegen.yml",
    "hardhat:node": "npx hardhat node",
    "hardhat:deploy-local": "npx hardhat run src/web3/scripts/deploy.js --network localhost",
    "hardhat:deploy-mumbai": "npx hardhat run src/web3/scripts/deploy.js --network mumbai",
    "hardhat:deploy-goerli": "npx hardhat run src/web3/scripts/deploy.js --network goerli",

    "hardhat:deploy-arbitrum": "npx hardhat run src/web3/scripts/deploy.js --network arbitrum",
    "copy-server": "cp -rf ../admin/src/server/ ./src/server/ && cp ../admin/adminSettings.json ./adminSettings.json && cp -rf ../admin/src/templates/ ./src/templates/ ",
    "generate-prisma": "prisma generate --schema ../admin/prisma/schema.prisma",
    "setup": "yarn copy-server && yarn generate-prisma && yarn codegen"
  },
